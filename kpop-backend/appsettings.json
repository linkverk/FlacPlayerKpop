{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MusicSettings": {
    "MusicDirectory": "music",
    "MaxFileSize": 104857600,
